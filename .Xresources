Xft.dpi: 96
XTerm*renderFont: true
XTerm*faceName: xft:Mononoki Nerd Font,
XTerm*faceSize: 18
XTerm*utf8: 2
XTerm*locale: true
XTerm*termName: xterm-256color

UXTerm*renderFont: true
UXTerm*faceName: xft:Mononoki Nerd Font, \
UXTerm*faceSize: 18
UXTerm*utf8: 2
UXTerm*locale: true
UXTerm*termName: xterm-256color

XTerm.vt100.translations: #override \n\
  Ctrl <Key> j: smaller-vt-font() \n\
  Ctrl <Key> k: larger-vt-font() \n\
  Ctrl Shift <Key>C: copy-selection(CLIPBOARD) \n\
  Ctrl Shift <Key>V: insert-selection(CLIPBOARD)

UXTerm.vt100.translations: #override \n\
  Ctrl <Key> j: smaller-vt-font() \n\
  Ctrl <Key> k: larger-vt-font() \n\
  Ctrl Shift <Key>C: copy-selection(CLIPBOARD) \n\
  Ctrl Shift <Key>V: insert-selection(CLIPBOARD)

! Every shell is a login shell by default (for inclusion of all necessary environment variables)
XTerm*loginshell: true
UXTerm*loginshell: true

! A lot of scrollback...
XTerm*savelines: 16384
UXTerm*savelines: 16384

! double-click to select whole URLs :D
XTerm*charClass: 33:48,36-47:48,58-59:48,61:48,63-64:48,95:48,126:48
UXTerm*charClass: 33:48,36-47:48,58-59:48,61:48,63-64:48,95:48,126:48

